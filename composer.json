{
    "name": "agentur1601com/gsd-special-announcements",
    "type": "contao-bundle",
    "description": "Extends the news-bundle of contao with fields that simplify the implementation of SpecialAnnouncements from google structured data",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Frank Ficzak",
            "email": "ff@1601.com",
            "homepage": "https://github.com/ff1601com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "conflict": {
        "contao/core": "^4.1",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "agentur1601com\\GsdSpecialAnnouncements\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "agentur1601com\\GsdSpecialAnnouncements\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/1601com/GsdSpecialAnnouncements/issues"
    }
}
